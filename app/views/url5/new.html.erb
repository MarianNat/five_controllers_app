<div class="form_wrap">
  <h3>Add profile data</h3>
  <div class="form">
    <%= form_for(@user, url: url5_create_path(@user)) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :about %>
      <%= f.text_field :about %>
      <%= f.submit 'Send' %>
    <% end %>
  </div>
  <%= link_to 'Back', url5_index_path %>
</div>